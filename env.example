# Google Sheets API Configuration
# Get your Google Sheets ID from the URL: https://docs.google.com/spreadsheets/d/SHEET_ID_HERE/edit
GOOGLE_SHEET_ID=your_google_sheet_id_here

# Path to your Google Service Account credentials JSON file
# Download from Google Cloud Console > APIs & Services > Credentials
GOOGLE_APPLICATION_CREDENTIALS=cred/credentials.json

# Enable debug mode for testing (shows distribution over 1000 tries)
DEBUG=False
